{
    "manifest_version": 3,
    "name": "Browser Tools",
    "version": "1.0",
    "description": "Manage bookmarks, browsing history, and cookies from a single interface.",
    "permissions": [
        "browsingData",
        "cookies",
        "tabs",
        "scripting"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "homepage_url": "https://docs.google.com/document/d/1qGj9FFCmUCMrwBD4xx5iXPJxqOuoqx7aWZ2lHViMuFc/edit?usp=sharing",
    "icons": {
        "16": "icons/icon-16.png",
        "48": "icons/icon-48.png",
        "128": "icons/icon-128.png"
    }
}